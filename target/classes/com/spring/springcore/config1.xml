<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">
                            
    <bean class="com.spring.springcore.Certi" name="cer">
        <constructor-arg value="Python Programing Language"/>
    </bean>

    <bean id="person" class="com.spring.springcore.Person">
        <constructor-arg value="Hruta"/>
        <constructor-arg value="12"/>
       <constructor-arg ref="cer"/>
    </bean>
    
    <bean class="com.spring.springcore.Addition" name="add">
    	<constructor-arg value="12" type="double"/>
    	<constructor-arg value="32" type="double"/>
    </bean>
    



</beans>
